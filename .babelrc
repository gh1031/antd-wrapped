{
  "presets": ["@babel/preset-react"],
  "plugins": [
    ["import", {
      "libraryName": "antd",
      "libraryDirectory": "es",
      "style": "css"
    }],
    ["@babel/plugin-proposal-decorators", { "legacy": true, "loose": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
  ]
}