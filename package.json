{
  "name": "antd-wrapped",
  "version": "0.0.2",
  "description": "A UI components based on antd",
  "main": "index.js",
  "author": "gh1031",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development concurrently \"npm:dll\" \"npm run dev\"",
    "build": "NODE_ENV=production concurrently \"npm:clean\" \"npm:dll\" \"npm:prod\" ",
    "dev": " webpack-dev-server --config ./build/webpack.config.dev.js ",
    "prod": "webpack --config ./build/webpack.config.prod.js",
    "dll": "webpack --config ./build/webpack.config.dll.js",
    "optimization:dev": "OPTIMIZATION=true npm run dev",
    "optimization:prod": "OPTIMIZATION=true npm run prod",
    "clean": "rm -rf ./dist/lib"
  },
  "dependencies": {
    "antd": "^3.16.1",
    "css-loader": "^2.1.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "style-loader": "^0.23.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "hoist-non-react-statics": "^3.3.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "mini-css-extract-plugin": "^0.6.0",
    "styled-components": "^4.2.0",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "cross-env ESLINT_ENV=GIT eslint"
    ]
  }
}
