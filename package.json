{
  "name": "antd-wrapped",
  "version": "0.1.0",
  "description": "A UI components based on antd",
  "main": "lib/index.js",
  "author": "gh1031",
  "license": "MIT",
  "files": [
    "lib",
    "docs",
    "es",
    "dist"
  ],
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --config scripts/webpack.config.dev.js",
    "type": "tsc --project ./components/tsconfig.json --emitDeclarationOnly",
    "build:umd": "webpack --config scripts/webpack.config.prod.js",
    "build:es": "NODE_ENV=production MODULE_ENV=es babel components --out-dir es --extensions \".js,.jsx,.ts,.tsx\"",
    "build:cjs": "NODE_ENV=production MODULE_ENV=cjs babel components --out-dir lib --extensions \".js,.jsx,.ts,.tsx\"",
    "build:website": "concurrently \"npm:clean\" \"npm:dll\" \"npm:prod\" ",
    "build:all": "npm run clean && npm run type && concurrently \"npm:build:umd\" \"npm:build:es\" \"npm:build:cjs\"",
    "deploy": "npm run build && npm publish",
    "prod": "NODE_ENV=production npm run clean && webpack --config scripts/webpack.config.prod.js",
    "dll": "webpack --config scripts/webpack.config.dll.js",
    "optimization:dev": "OPTIMIZATION=true npm run dev",
    "optimization:prod": "OPTIMIZATION=true npm run prod",
    "clean": "rm -rf dist es lib",
    "lint": "eslint  --ext .js,.jsx,.ts,.tsx src/** components/**",
    "lint:fix": "eslint  --ext .js,.jsx,.ts,.tsx, --fix src/* components/*",
    "test": "jest",
    "cm": "git-cz"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "antd": "^4.4.1",
    "axios": "^0.19.0",
    "core-js": "^3.2.1",
    "lodash.debounce": "^4.0.8",
    "lodash.isnil": "^4.0.0",
    "lodash.omitby": "^4.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "spin.js": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.0",
    "@types/redux": "^3.6.0",
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.13.0",
    "commitizen": "^4.0.3",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.0",
    "hoist-non-react-statics": "^3.3.0",
    "html-webpack-include-assets-plugin": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.2.0",
    "jest": "^26.1.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.1.6",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "style-loader": "^0.23.1",
    "typescript": "^3.9.6",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "browserslist": {
    "production": [
      "> 1%, last 3 versions",
      "ie 10"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "cross-env ESLINT_ENV=GIT eslint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
