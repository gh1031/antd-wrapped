{
  "name": "antd-wrapped",
  "version": "0.1.0",
  "description": "A UI components based on antd",
  "main": "lib/index.js",
  "author": "gh1031",
  "license": "MIT",
  "files": [
    "lib",
    "docs"
  ],
  "scripts": {
    "start": "concurrently \"npm:dll\" \"npm run dev\"",
    "test": "jest",
    "build": "NODE_ENV=development rm -rf lib && webpack",
    "build:esm": "NODE_ENV=production MODULE_ENV=esm babel components --out-dir es",
    "build:cjs": "NODE_ENV=production MODULE_ENV=cjs babel components --out-dir cjs",
    "website": "concurrently \"npm:clean\" \"npm:dll\" \"npm:prod\" ",
    "dev": " npm run dll && webpack-dev-server --config ./build/webpack.config.dev.js ",
    "prod": "npm run dll && webpack --config ./build/webpack.config.prod.js",
    "dll": "webpack --config ./build/webpack.config.dll.js --silent",
    "optimization:dev": "OPTIMIZATION=true npm run dev",
    "optimization:prod": "OPTIMIZATION=true npm run prod",
    "clean": "rm -rf ./dist/lib",
    "deploy": "npm run build && npm publish",
    "lint": "eslint  --ext .js,.jsx,.ts,.tsx src/** components/**",
    "lint:fix": "eslint  --ext .js,.jsx,.ts,.tsx, --fix src/* components/*"
  },
  "peerDependencies": {
    "react": ">16.8.6",
    "react-dom": ">16.8.6",
    "antd": ">3.16.1"
  },
  "dependencies": {
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.1",
    "@types/react-router-dom": "^5.1.0",
    "axios": "^0.19.0",
    "core-js": "^3.2.1",
    "dotenv": "^8.1.0",
    "dotenv-expand": "^5.1.0",
    "jest": "^24.8.0",
    "lodash": "^4.17.11",
    "lodash.debounce": "^4.0.8",
    "lodash.isnil": "^4.0.0",
    "lodash.omitby": "^4.6.0",
    "prop-types": "^15.7.2",
    "react-redux": "^7.0.3",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "spin.js": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "antd": "^3.19.7",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.0",
    "hoist-non-react-statics": "^3.3.0",
    "html-webpack-include-assets-plugin": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.1.6",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "browserslist": {
    "production": [
      "> 1%, last 3 versions",
      "ie 10"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "cross-env ESLINT_ENV=GIT eslint"
    ]
  }
}
